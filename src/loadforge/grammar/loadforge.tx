TestFile:
    test=Test
;

Test:
    'test' name=STRING '{'
        (environment=Environment)?
        ('target' target=Target)?
    '}'
;

Target:
    ref=Ref | value=STRING
;

Environment:
    'environment' '{'
        envVars*=EnvVar
    '}'
;

EnvVar:
    name=ID '=' value=EnvCall
;

EnvCall:
    'env' '(' key=STRING ')'
;

Ref:
    name=ID
;
